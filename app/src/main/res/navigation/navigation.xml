            android:id="@+id/action_menu_to_news"
            app:destination="@id/entertaimentNews"
            app:popUpTo="@id/menuNewsFragment"
            app:popUpToInclusive="true" />
        <argument
            android:name="email"
            android:defaultValue="Login"
            app:argType="string" />
        <argument
            android:name="emailRegister"
            android:defaultValue="Register"
            app:argType="string" />
    </fragment>

    <fragment
        android:id="@+id/articleNewsFragment"
        android:name="com.example.newsappmvvm.ui.fragments.ArticleNewsFragment"
        android:label="Article News">

        <argument
            android:name="article"
            app:argType="com.example.newsappmvvm.model.Article" />
    </fragment>


    <!--//Fragment menu-->

    <fragment
        android:id="@+id/loginNewsFragment"
        android:name="com.example.newsappmvvm.ui.fragments.LoginNewsFragment"
        android:label="LoginNewsFragment">
        <action
            android:id="@+id/action_login_to_breakingnew"
            app:destination="@id/menuNewsFragment"
            app:popUpTo="@id/loginNewsFragment"
            app:popUpToInclusive="true">
            <argument
                android:name="email"
                app:argType="string" />
            <argument
                android:name="password"
                app:argType="string" />
        </action>
    </fragment>
    <fragment
        android:id="@+id/registerNewsFragment"
        android:name="com.example.newsappmvvm.ui.fragments.RegisterNewsFragment"
