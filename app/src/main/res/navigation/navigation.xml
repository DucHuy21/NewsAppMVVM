            android:id="@+id/action_register_to_breakingnew"
            app:destination="@id/menuNewsFragment"
            app:popUpTo="@id/registerNewsFragment"
            app:popUpToInclusive="true">
            <argument
                android:name="user"
                app:argType="string" />
            <argument
                android:name="email"
                app:argType="string" />
            <argument
                android:name="password"
                app:argType="string" />
        </action>
    </fragment>


